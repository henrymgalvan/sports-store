@model ProductsListViewModel

@foreach (var item in Model.Products)
{
    @Html.Partial("ProductSummary", item)
}

<div page-model="@Model.PageInfo" page-action="list" class="pull-right button-group" page-classes-enabled="true" page-class="btn" page-class-selected="btn-primary" page-class-normal="btn-default" page-url-category="@Model.CurrentCategory"></div>